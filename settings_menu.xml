<jentemplate>
    <mysettings>
        <plugin>
            <title>[B][COLOR snow]•[/COLOR][COLOR lime]•[/COLOR][COLORlightslategray]JEN: Customization [/COLOR][/B]</title>
            <meta>
                <summary>Open the Settings for the addon on the Customization tab</summary>
            </meta>
            <mysettings>0/0</mysettings>
        </plugin>

        <plugin>
            <title>[B][COLOR snow]•[/COLOR][COLOR lime]•[/COLOR][COLORlightslategray]JEN: Cache [/COLOR][/B]</title>
            <meta>
                <summary>Open the Settings for the addon on the General tab</summary>
            </meta>
            <mysettings>1/0</mysettings>
        </plugin>

        <plugin>
            <title>[B][COLOR snow]•[/COLOR][COLOR lime]•[/COLOR][COLORlightslategray]JEN: Scraper Setup [/COLOR][/B]</title>
            <meta>
                <summary>Open the Settings for the addon on the General tab</summary>
            </meta>
            <mysettings>2/0</mysettings>
        </plugin>

        <plugin>
            <title>[B][COLOR snow]•[/COLOR][COLOR lime]•[/COLOR][COLORlightslategray]JEN: Resolver Setup [/COLOR][/B]</title>
            <meta>
                <summary>Open the Settings for the addon on the Resolvers tab</summary>
            </meta>
            <mysettings>3/0</mysettings>
        </plugin>
        
        <plugin>
            <title>[B][COLOR snow]•[/COLOR][COLOR lime]•[/COLOR][COLORlightslategray]JEN: API Settings [/COLOR][/B]</title>
            <meta>
                <summary>Open the API Settings if you wish to add your own API Keys to Arrakis</summary>
            </meta>
            <mysettings>4/0</mysettings>
        </plugin>

        <plugin>
            <title>[B][COLOR snow]•[/COLOR][COLOR lime]•[/COLOR][COLORlightslategray]REAL-DEBRID: Re-Authorize (ResolveURL) [/COLOR][/B]</title>
            <meta>
                <summary>(Re)Authorize Real-Debrid in the Resolve URL Addon</summary>
            </meta>
