<jentemplate>
    <mysettings>
        <plugin>
            <title>[B][COLOR snow]•[/COLOR][COLOR lime]•[/COLOR][COLORlightslategray] Customization [/COLOR][/B]</title>
            <meta>
                <summary>Open the Settings for the addon on the Customization tab</summary>
            </meta>
            <mysettings>0/0</mysettings>
        </plugin>

        <plugin>
            <title>[B][COLOR snow]•[/COLOR][COLOR lime]•[/COLOR][COLORlightslategray] Cache [/COLOR][/B]</title>
            <meta>
                <summary>Open the Settings for the addon on the General tab</summary>
            </meta>
            <link>plugin://script.module.resolveurl/?mode=reset_cache</link>
        </plugin>

        <plugin>
            <title>[B][COLOR snow]•[/COLOR][COLOR lime]•[/COLOR][COLORlightslategray] Scraper Setup [/COLOR][/B]</title>
            <meta>
                <summary>Open the Settings for the addon on the General tab</summary>
            </meta>
            <link>plugin://plugin.video.jaws/?mode=ScraperSettings</link>
        </plugin>

        <plugin>
            <title>[B][COLOR snow]•[/COLOR][COLOR lime]•[/COLOR][COLORlightslategray] ResolveURL Setup [/COLOR][/B]</title>
            <meta>
                <summary>Open the Settings for the addon on the Resolvers tab</summary>
            </meta>
            <link>plugin://plugin.video.jaws/?mode=ResolverSettings</link>
        </plugin>
        
        <plugin>
            <title>[B][COLOR snow]•[/COLOR][COLOR lime]•[/COLOR][COLORlightslategray] REAL-DEBRID:Authorize (ResolveURL) [/COLOR][/B]</title>
            <meta>
                <summary>(Re)Authorize Real-Debrid in the Resolve URL Addon</summary>
            </meta>
            <link>plugin://script.module.resolveurl/?mode=auth_rd</link>
        </plugin>
        
        <plugin>
            <title>[B][COLOR snow]•[/COLOR][COLOR lime]•[/COLOR][COLORlightslategray] Premiumizer:Authorize (ResolveURL) [/COLOR][/B]</title>
            <meta>
                <summary>Authorize Premiumize in the Resolve URL Addon</summary>
            </meta>
            <link>plugin://script.module.resolveurl/?mode=auth_pm ,return,return</link>
        </plugin>
       
        <plugin>
            <title>[B][COLOR snow]•[/COLOR][COLOR lime]•[/COLOR][COLORlightslategray] Authenticate Trakt[/COLOR][/B]</title>
            <meta>
                <summary>Authorize Trakt</summary>
            </meta>
            <link>plugin://script.module.trakt/?action=authTrakt ,return,return</link>
        </plugin>       
        
        <plugin>
            <title>[B][COLOR snow]•[/COLOR][COLOR lime]•[/COLOR][COLORlightslategray] API Settings [/COLOR][/B]</title>
            <meta>
                <summary>Open the API Settings if you wish to add your own API Keys to Arrakis</summary>
            </meta>
            <mysettings>4/0</mysettings>
        </plugin>

        <item>
            <title>[B][COLOR snow]•[/COLOR][COLOR lime]•[/COLOR][COLORlightslategray] PAIRING: Pair My Connection[/COLOR][/B]</title>
            <meta>
                <summary>Pair with a selection of sites that require pairing for non-premium access</summary>
            </meta>
            <pairwith>pairlist</pairwith>
        </item>
        
        <plugin>
            <title>[B][COLOR snow]•[/COLOR][COLOR lime]•[/COLOR][COLORlightslategray] Settings[/COLOR][/B]</title>
            <meta>
            <link>plugin://plugin.video.jaws/?action=openSettings</link>
        </plugin>
        
        <item>
            <title>[B][COLOR snow]•[/COLOR][COLOR lime]•[/COLOR][COLORlightslategray] YOUTUBE: Live and CAMS Fix[/COLOR][/B]</title>
            <meta>
                <summary>Apply fix for YouTube Live playback issues and if you have problems with CAMS</summary>
            </meta>
            <ytdash></ytdash>
        </item>
    </mysettings>
</jentemplate>